{
  "name": "http-server",
  "version": "0.11.1",
  "description": "A simple zero-configuration command-line http server",
  "main": "./lib/http-server",
  "repository": {
    "type": "git",
    "url": "git://github.com/http-party/http-server.git"
  },
  "keywords": [
    "cli",
    "command"
  ],
  "scripts": {
    "start": "node ./bin/http-server",
    "pretest": "eslint bin/http-server lib/ test",
    "test": "vows --spec --isolate"
  },
  "files": [
    "lib",
    "bin"
  ],
  "eslintConfig": {
    "env": {
      "node": true,
      "es6": true
    },
    "extends": "populist"
  },
  "dependencies": {
    "basic-auth": "^1.0.3",
    "charset": "^1.0.1",
    "colors": "^1.3.3",
    "commander": "^3.0.0",
    "corser": "^2.0.1",
    "he": "^1.2.0",
    "http-proxy": "^1.17.0",
    "mime": "^2.4.4",
    "on-finished": "^2.3.0",
    "opener": "^1.5.1",
    "portfinder": "^1.0.22",
    "secure-compare": "3.0.1",
    "union": "~0.5.0",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "eslint": "^6.0.1",
    "eslint-config-populist": "^4.2.0",
    "request": "^2.88.0",
    "vows": "~0.7.0"
  },
  "bugs": {
    "url": "https://github.com/http-party/http-server/issues"
  },
  "license": "MIT",
  "preferGlobal": "true",
  "bin": {
    "http-server": "./bin/http-server",
    "hs": "./bin/http-server"
  },
